{
  "name": "bitcoinwallet",
  "version": "0.0.1",
  "description": "Simple mobile Bitcoin & Ethereum wallet",
  "author": "Martin Litvaj <kamahl19@gmail.com> (http://litvaj.com)",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.3.0"
  },
  "scripts": {
    "cleanup":
      "watchman watch-del-all && rm -rf node_modules && yarn install && rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-*",
    "format": "prettier --write '{src,__{tests,mocks}__}/**/*.js'",
    "postinstall":
      "sed -i -e \"s/var Script = function Script(from) {/var Script = function Script(from) { Address = require('..\\/address');/g\" ./node_modules/bitcore-lib/lib/script/script.js",
    "run-ios": "react-native run-ios",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "babel-jest": "^22.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-native": "^4.0.0",
    "eslint": "^4.16.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.1.4",
    "prettier": "^1.10.2",
    "react-devtools": "^3.0.0",
    "react-test-renderer": "^16.2.0",
    "redux-logger": "^3.0.6"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "big.js": "^5.0.3",
    "bip21": "^2.0.1",
    "bitcore-wallet-client": "^6.5.0",
    "bs58check": "^2.1.1",
    "cryptocompare": "^0.4.0",
    "lodash.flattendeep": "^4.4.0",
    "lodash.memoize": "^4.1.2",
    "lodash.reduce": "^4.6.0",
    "moment": "^2.20.1",
    "node-libs-browser": "^2.1.0",
    "prop-types": "^15.6.0",
    "rc-form": "^1.5.0",
    "react": "^16.2.0",
    "react-native": "0.52.0",
    "react-native-camera": "^0.13.0",
    "react-native-dropdownalert": "^3.1.2",
    "react-native-keyboard-manager": "^4.0.13-5",
    "react-native-material-textfield": "^0.12.0",
    "react-native-qrcode-scanner": "^0.0.29",
    "react-native-qrcode-svg": "^5.0.6",
    "react-native-radio-buttons": "^1.0.0",
    "react-native-randombytes": "^3.0.0",
    "react-native-svg": "^5.5.1",
    "react-native-vector-icons": "^4.5.0",
    "react-navigation": "^1.0.0-beta.27",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-persist": "^5.5.0",
    "redux-saga": "^1.0.1",
    "reselect": "^3.0.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
